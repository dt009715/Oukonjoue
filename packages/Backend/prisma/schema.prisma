generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Institution {
  id      Int     @id @default(autoincrement())
  name    String
  address String
  phone String
  mail String @unique
  category String
  createdAt DateTime @default(now())
}

model Artists {
  id      Int     @id @default(autoincrement())
  name    String
  address String
  phone String
  mail String @unique
  category String
  createdAt DateTime @default(now())
}